table {
    float: right;
    margin-right: 0 !important;
    background-color: #dbdbdb;
    color: #24292e;
    width: 20%;
    margin: 30px;
    margin-top: 0;
    border: 5px solid #dbdbdb;
}

@keyframes an5 {
    from {
        box-shadow: 0 0 0 #dbdbdb;
    }
    to {
        box-shadow: 0 0 10px #dbdbdb;
    }
}

@keyframes an6 {
    from {
        box-shadow: 0 0 10px #dbdbdb;
    }
    to {
        box-shadow: 0 0 0 #dbdbdb;
    }
}

table:not(:hover) {
    box-shadow: 0 0 0 #dbdbdb;
    animation: an6 0.5s;
}

table:hover {
    animation: an5 0.5s;
    box-shadow: 0 0 10px #dbdbdb;
}

